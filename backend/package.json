  "version": "1.0.0",
  "description": "Backend API for AI-Powered Learning Management System",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
